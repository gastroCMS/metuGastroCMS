    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "msw": "^2.10.4",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5",
